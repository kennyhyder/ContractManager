{
  "app": {
    "name": "Contract Management System",
    "version": "1.0.0",
    "environment": "test"
  },
  "features": {
    "contracts": true,
    "templates": true,
    "approvals": true,
    "analytics": true,
    "marketplace": true,
    "collaboration": true,
    "encryption": true
  },
  "limits": {
    "maxFileSize": 10485760,
    "maxContractsPerUser": 1000,
    "maxUsersPerContract": 50
  },
  "ui": {
    "theme": "light",
    "dateFormat": "MM/DD/YYYY",
    "currency": "USD",
    "language": "en"
  }
}